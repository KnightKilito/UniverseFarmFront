<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.MetaverseMaker.UniverseFarm.mapper.OrderItemVOMapper" >
<select id="queryIDOrderItemVO" resultType="com.MetaverseMaker.UniverseFarm.entity.OrderItemVO">
    select * from `v_orderitem-soilinfo` where order_id = #{order_id}

</select>
    <select id="queryAllOrderItemVO" resultType="com.MetaverseMaker.UniverseFarm.entity.OrderItemVO">
        select * from `v_orderitem-soilinfo` where 1 = 1
    </select>
</mapper>
